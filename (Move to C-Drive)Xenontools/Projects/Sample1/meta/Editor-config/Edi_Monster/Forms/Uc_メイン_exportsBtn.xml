<?xml version="1.0" encoding="Shift_JIS" ?>
                                    <!-- ↓このバージョン番号の説明は
                                           Aa_Editor.xml参照 -->
<codefile-controls codefile-version="2.0">


    <!--
      このコントロールの紹介
    ┏━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┓
        ■
      ■■■ 書出しボタン
        ■
                                        [記号] Uc:メイン_exportsBtn;
                                        [種類] ボタン（C:Btn;）
                                        [画面] メイン画面（Ufo:メイン;）
                                                    （Forms/Ufo_メイン.csv）

                別フォルダーに
                ＣＳＶファイルを書き出すボタンです。

    ├──────────────────────────────────┤
      【機能】
                                ・CSVファイルを書き出す。
    ┗━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━┛
    -->
    <control>


        <!--
          イベントの設定
        ┌──────────────────────────────────┐
            ■
          ■  ■ このボタンを押下した時（①）。
            ■

              （１）現在編集中の 敵テーブル（②）を、
                    敵ゲームデータテーブル（③）に上書きします。
                    
              （２）現在編集中の 敵作戦テーブル（④）を、
                    敵作戦ゲームデータテーブル（⑤）に上書きします。
        -->
        <event name="Se:ボタン押下時;"><!-- ① -->
            <fnc name="Sf:CSV書出;"><!-- （１） -->
                <arg name="Pm:name-table-src;"         value="Ut:敵;" /><!-- ② -->
                <arg name="Pm:name-table-dst;"         value="Ut:敵ゲームデータ;" /><!-- ③ -->
            </fnc>
            <fnc name="Sf:CSV書出;"><!-- （２） -->
                <arg name="Pm:name-table-src;"         value="Ut:敵作戦;" /><!-- ④ -->
                <arg name="Pm:name-table-dst;"         value="Ut:敵作戦ゲームデータ;" /><!-- ⑤ -->
            </fnc>
        </event>
        <!--
        └──────────────────────────────────┘
        -->
    </control>


</codefile-controls>
